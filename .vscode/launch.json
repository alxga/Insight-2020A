    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: GTFS Schedule Viewer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/schedule_viewer.py",
            "console": "integratedTerminal",
            "args": [
              "~/data/MBTA_GTFS_20200126.zip"
            ]
        },
        {
          "name": "Python: Flask",
          "type": "python",
          "request": "launch",
          "module": "flask",
          "env": {
              "FLASK_APP": "frontend_app.py",
              "FLASK_ENV": "development",
              "FLASK_DEBUG": "0"
          },
          "args": [
              "run",
              "--no-debugger",
              "--no-reload"
          ],
          "jinja": true
      },
    ]
}